<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.songmj.kpi.mapper.KpiAuditMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.songmj.kpi.entity.KpiAudit">
        <id column="ka_id" property="kaId" />
        <result column="kd_id" property="kdId" />
        <result column="ka_result" property="kaResult" />
        <result column="ka_msg" property="kaMsg" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ka_id AS kaId, kd_id AS kdId, ka_result AS kaResult, ka_msg AS kaMsg
    </sql>

</mapper>
