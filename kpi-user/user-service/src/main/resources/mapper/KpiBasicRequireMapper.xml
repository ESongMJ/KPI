<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.songmj.kpi.mapper.KpiBasicRequireMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.songmj.kpi.entity.KpiBasicRequire">
        <id column="kbr_id" property="kbrId" />
        <result column="kbr_type" property="kbrType" />
        <result column="kbr_level" property="kbrLevel" />
        <result column="kbr_teach" property="kbrTeach" />
        <result column="kbr_research" property="kbrResearch" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        kbr_id AS kbrId, kbr_type AS kbrType, kbr_level AS kbrLevel, kbr_teach AS kbrTeach, kbr_research AS kbrResearch
    </sql>

</mapper>
