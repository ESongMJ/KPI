<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.songmj.kpi.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.songmj.kpi.entity.User">
        <id column="user_id" property="userId" />
        <result column="user_name" property="userName" />
        <result column="user_sex" property="userSex" />
        <result column="user_age" property="userAge" />
        <result column="user_birthday" property="userBirthday" />
        <result column="user_introduce" property="userIntroduce" />
        <result column="user_img" property="userImg" />
        <result column="user_number" property="userNumber" />
        <result column="user_password" property="userPassword" />
        <result column="user_college" property="userCollege" />
        <result column="user_r_o" property="userRO" />
        <result column="user_pro_title" property="userProTitle" />
        <result column="role_id" property="roleId" />
        <result column="kbr_id" property="kbrId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id AS userId, user_name AS userName, user_sex AS userSex, user_age AS userAge, user_birthday AS userBirthday, user_introduce AS userIntroduce, user_img AS userImg, user_number AS userNumber, user_password AS userPassword, user_college AS userCollege, user_r_o AS userRO, user_pro_title AS userProTitle, role_id AS roleId, kbr_id AS kbrId
    </sql>

</mapper>
