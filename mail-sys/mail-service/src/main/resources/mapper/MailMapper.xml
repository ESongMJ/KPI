<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.songmj.kpi.mapper.MailMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.songmj.kpi.entity.Mail">
        <id column="mail_id" property="mailId" />
        <result column="user_id" property="userId" />
        <result column="mail_title" property="mailTitle" />
        <result column="mail_content" property="mailContent" />
        <result column="mail_date" property="mailDate" />
        <result column="mail_status" property="mailStatus" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        mail_id AS mailId, user_id AS userId, mail_title AS mailTitle, mail_content AS mailContent, mail_date AS mailDate, mail_status AS mailStatus
    </sql>

</mapper>
